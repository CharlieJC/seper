{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:cypress/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react", 
        "cypress"
    ],
    "rules": {
        "linebreak-style":0,
        "no-console": "off",
        "jest/expect-expect": "off"
    },
    "globals": {
        "cy": true
    }
}
